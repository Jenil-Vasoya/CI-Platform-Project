<script>
      function searchMission(pg) {
        console.log("abc");
        var search = $('input[name="search"]').val();

        if (pg == undefined) {
            pg = 1;
        }

        debugger;
        $.ajax({
            url: "/Admin/Search",
            method: "POST",
            data: {
                "search": search,
                "pg": pg
            },
            success: function (data) {
                $('#users').html(data);
            },
            error: function (request, error) {
                console.log(error);
            }
        })
       
    }
</script>


