<div id="missionpage">
   

    <div class="row gx-0">
        <table class="table table-hover  table-bordered" style="width: 99%;margin-inline: auto;">
            <thead>
                <tr class="table-light" style="height: 70px;">
                    <th scope="col" class="thforcmstable">Mission Title</th>
                    <th scope="col" class="thforcmstable">Mission Type</th>
                    <th scope="col" class="thforcmstable">Start Date</th>
                    <th scope="col" class="thforcmstable">End Date</th>
                    <th scope="col" class="thforcmstable">Action</th>
                </tr>
            </thead>
            @foreach (var item in @ViewBag.MissionList)
            {
                <tbody class="table-border">
                    <tr>
                        <td>@item.Title</td>
                        <td>@item.MissionType</td>
                        <td>@item.StartDate</td>
                        <td>@item.EndDate</td>
                        <td>
                             <button type="button" class="btn p-0" onclick="editMission('nav-mission',@item.MissionId)"><i class="bi bi-pencil-square" style="color: coral;"></i></button>
                             <button type="button" class="btn p-0" data-bs-toggle="modal" data-bs-target="#exampleModalDeleteTime-@item.MissionId"><i class="bi bi-trash ms-3"></i></button>


                                                 <!-- Modal For Delete Mission -->
                                                    <div class="modal fade" id="exampleModalDeleteTime-@item.MissionId" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">

                                                        <div class="modal-dialog modal-dialog-centered">
                                                            <div class="modal-content">
                                                                <div class="modal-header">
                                                                    <h1 class="modal-title fs-5" id="exampleModalLabel">
                                                                        Are you sure for delete this Mission
                                                                    </h1>
                                                                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                                                </div>

                                                                <form method="post">

                                                                    <div class="modal-footer">
                                                                        <button type="button" class="btn btn-secondary bg-transparent border border-2 rounded-pill text-secondary" data-bs-dismiss="modal">Cancel</button>
                                                                        <button type="submit" onclick="deleteMission(@item.MissionId,event)" class="btn applyButton" data-bs-dismiss="modal">Delete</button>
                                                                    </div>
                                                                </form>
                                                            </div>
                                                        </div>
                                                    </div>

                        </td>
                    </tr>
                </tbody>
            }
        </table>
    </div>

    <nav aria-label="Page navigation example">
        <ul class="pagination justify-content-center" onchange="searchMission()">
            <li class="page-item">
                <button class="page-link" type="submit" aria-label="Previous" onclick="searchMission(1,'mission')">
                    <span aria-hidden="true">&laquo;</span>
                </button>
            </li>
            @{
                double totalpages = ViewBag.ttlMissions;

                for (int i = 1; i <= totalpages; i++)
                {
                    @if (i == ViewBag.pg_no)
                    {
                        <li class="page-item active">
                            <button class="page-link" type="submit" onclick="searchMission(@i,'mission')">@i</button>

                        </li>
                    }
                    else
                    {
                        <li class="page-item">
                            <button class="page-link" type="submit" onclick="searchMission(@i,'mission')">@i</button>
                        </li>
                    }


                }

                <li class="page-item">
                    <button class="page-link" type="submit" aria-label="Next" onclick="searchMission(@totalpages,'mission')">
                        <span aria-hidden="true">&raquo;</span>
                    </button>
                </li>
            }
        </ul>
    </nav>
</div>

