    }
});


function addSkillMission(event) {
    
  var skillId = event.target.id;
  var skillName = event.target.textContent;

  // Check if the event was triggered in the source div
  if ($(event.target).closest("#sourceDiv").length) {
    // Check if the skill already exists in the destination div
    if ($("#destinationDiv #" + skillId).length == 0) {
      // Add the skill to the destination div
      $("#destinationDiv").append("<li id='" + skillId + "' class='skill list-unstyled cursor' onclick='addSkillMission(event)'>" + skillName + "</li>");
      // Check if the skill already exists in the select element
      if ($("#addMissionSkill option[value='" + skillId + "']").length == 0) {
        // Add the skill to the select element
        $("#addMissionSkill").append("<option value='" + skillId + "'>" + skillName + "</option>");
      }
    }
  } else if ($(event.target).closest("#destinationDiv").length) { // Check if the event was triggered in the destination div
    // Remove the skill from the destination div
    $("#" + skillId).remove();
    // Remove the skill from the select element
    $("#addMissionSkill option[value='" + skillId + "']").remove();
  }
}

