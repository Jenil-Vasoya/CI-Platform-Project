 <script>
   
    $(document).ready(function() {
            $("#addMissionType").change(function() {
                var selectedType = $(this).val();
                if (selectedType === "Goal") {
                  $("#addMissionStartDate").prop("disabled", true);
                  $("#addMissionEndDate").prop("disabled", true);
                  $("#addMissionSeat").prop("disabled", false);
                } else if (selectedType === "Time") {
                  $("#addMissionStartDate").prop("disabled", false);
                  $("#addMissionEndDate").prop("disabled", false);
                  $("#addMissionSeat").prop("disabled", true);
                }
            }).change();
            
            $("#editMissionType").change(function() {
                var selectedType = $(this).val();
                if (selectedType === "Goal") {
                  $("#editMissionStartDate").prop("disabled", true);
                  $("#editMissionEndDate").prop("disabled", true);
                  $("#editMissionSeat").prop("disabled", false);
                } else if (selectedType === "Time") {
                  $("#editMissionStartDate").prop("disabled", false);
                  $("#editMissionEndDate").prop("disabled", false);
                  $("#editMissionSeat").prop("disabled", true);
                }
            }).change(); // Trigger change event on page load
    });


   $("#images").change(function() {
    var files = this.files;
    var editMissionImageDiv = $("#image-preview");

    // Clear existing images from the div
    editMissionImageDiv.empty();

    // Display the newly selected images
    for (var i = 0; i < files.length; i++) {
        var file = files[i];
        var reader = new FileReader();

        reader.onload = function(e) {
            var img = $("<img>");
            img.attr("src", e.target.result);
            editMissionImageDiv.append(img);
        }

        reader.readAsDataURL(file);
